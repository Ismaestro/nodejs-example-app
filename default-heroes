CREATE SEQUENCE hero_id_seq;
CREATE TABLE heroes (hero_id smallint NOT NULL DEFAULT nextval('hero_id_seq'), name text, alter_ego text, likes integer, default_hero boolean);
ALTER SEQUENCE hero_id_seq OWNED BY heroes.hero_id;
